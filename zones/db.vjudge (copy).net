;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.vjudge.net. admin.example.net. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; Name servers
vjudge.net.    IN      NS      ns1.vjudge.net.
vjudge.net.    IN      NS      onu-HP-Notebook.

; A records for name servers
ns1             IN      A       192.168.1.2
onu-HP-Notebook.              IN      A       127.0.1.1
; Other A records
@               IN      A       139.162.66.202
www             IN      A       139.162.66.202

